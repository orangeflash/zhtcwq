/*  淡入淡出动画  */

@keyframes fadeIn {
  /*设置内容由显示变为隐藏*/

  0% {
    opacity: 0;
  }

  10% {
    opacity: 0.25;
  }

  20% {
    opacity: 0.5;
  }

  30% {
    opacity: 0.75;
  }

  40% {
    opacity: 1;
  }

  50% {
    opacity: 1;
  }

  60% {
    opacity: 1;
  }

  70% {
    opacity: 0.75;
  }

  80% {
    opacity: 0.5;
  }

  90% {
    opacity: 0.25;
  }

  100% {
    opacity: 0;
  }
}

